<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.jeanette.androidprojekt.Kategorier">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kategori 1"
        android:id="@+id/kat1"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="61dp" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kategori 2"
        android:id="@+id/kat2"
        android:layout_below="@+id/kat1"
        android:layout_alignStart="@+id/kat1" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kategori 3"
        android:id="@+id/kat3"
        android:layout_below="@+id/kat2"
        android:layout_centerHorizontal="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kategori 4"
        android:id="@+id/kat4"
        android:layout_below="@+id/kat3"
        android:layout_alignStart="@+id/kat3" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Venners uploads"
        android:id="@+id/katVenner"
        android:layout_below="@+id/kat4"
        android:layout_alignStart="@+id/kat4" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nye kategorier"
        android:id="@+id/nyKat"
        android:layout_below="@+id/katVenner"
        android:layout_alignStart="@+id/katVenner" />
</RelativeLayout>
